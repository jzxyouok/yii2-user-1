{
    "name": "yuncms/yii2-user",
    "description": "Flexible user registration and authentication module for Yii",
    "keywords": ["yii2", "yii2-user", "user management"],
    "type": "yii2-extension",
    "license": "MIT",
    "require": {
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-bootstrap": "^2.0.0",
        "xutl/yii2-authclient": "~1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "xutl/yii2-cropper-asset": "~1.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "yuncms\\user\\": ""
        }
    },
    "suggest": {
        "yuncms/yii2-vote": "Allows you to use vote",
        "yuncms/yii2-tag": "Allows you to use tag"
    },
    "extra": {
        "bootstrap": "yuncms\\user\\Bootstrap",
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-build|-patch)"
    }
}